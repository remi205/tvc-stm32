
## Makefile.am -- Process this file with automake to produce Makefile.in
lib_LIBRARIES = libipv4.a

AM_CFLAGS =-mcpu=cortex-m4 -mthumb -O3 -Wall -Wpointer-arith  -Winline -Wundef -g  -ffunction-sections -fdata-sections

CPPFLAGS += -Iinc/ -I../inc -I$(top_srcdir)/inc -I$(top_srcdir) -I$(top_srcdir)/STM32F4xx_HAL_Driver/inc
CFLAGS += -Iinc/ -I../inc -I$(top_srcdir)/inc -I$(top_srcdir) -I$(top_srcdir)/STM32F4xx_HAL_Driver/inc -I$(top_srcdir)/lwip/src/include -I$(top_srcdir)/lwip/src/include/ipv4

libipv4_a_SOURCES = autoip.c icmp.c igmp.c inet.c inet_chksum.c ip.c ip_addr.c ip_frag.c 


